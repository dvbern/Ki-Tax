  "scripts": {
    "rimraf": "rimraf",
    "typedoc": "typedoc",
    "clean": "npm run rimraf -- doc coverage dist .cache_loader",
    "prebuild": "check-engine && npm run clean && npm run set-version",
    "build": "npm run build:prod",
    "prebuild:ci": "npm run prebuild",
    "build:ci": "npm run build:prod -- --progress=false",
    "build:prod": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "preserver": "check-engine && npm run set-version",
    "server": "ng serve --disable-host-check",
    "server:ie": "ng serve --disable-host-check --configuration ie",
    "prelint": "check-engine",
    "lint": "concurrently 'npm:lint:ng' 'npm:lint:tsc' 'npm:lint:style'",
    "lint:ci": "concurrently 'npm:lint:ng:ci' 'npm:lint:tsc'",
    "lint:ng": "ng lint",
    "lint:style": "stylelint -s less \"**/*.less\"",
    "lint:style:fix": "stylelint --fix -s less \"**/*.less\"",
    "lint:ng:ci": "ng lint --format checkstyle > build/tslint-checkstyle-report.xml",
    "lint:tsc": "tsc --pretty --noEmit --project ./src/tsconfig.app.json && tsc --pretty --noEmit --project ./src/tsconfig.spec.json",
    "pretest": "check-engine",
    "test": "ng test",
    "pretest:ci": "npm run pretest",
    "test:ci": "ng test --progress=false --watch=false --code-coverage",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server",
    "start:ie": "npm run server:ie",
    "set-version": "node scripts/version-from-pom.js",
    "postinstall": "ngcc && npm run set-version && npm rebuild node-sass && mkdirp build"
  },
