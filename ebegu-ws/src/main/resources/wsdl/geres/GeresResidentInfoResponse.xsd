		<xs:sequence>
			<xs:element name="numberOfMessages" type="xs:nonNegativeInteger"/>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="messages" type="eCH-0020:eventBaseDelivery"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
