    "prebuild": "check-engine && npm run clean && npm run set-version",
    "build": "npm run build:prod",
    "prebuild:ci": "npm run prebuild",
    "build:ci": "npm run build:prod -- --progress=false",
    "build:prod": "ng build --configuration=production",
    "preserver": "check-engine && npm run set-version",
    "server": "ng serve",
    "prelint": "check-engine",
    "lint": "concurrently 'npm:lint:ng' 'npm:lint:tsc'",
    "lint:ng": "ng lint",
    "lint:tsc": "tsc --pretty --noEmit --project ./src/tsconfig.app.json && tsc --pretty --noEmit --project ./src/tsconfig.spec.json",
    "pretest": "check-engine",
    "test": "ng test",
    "pretest:ci": "npm run pretest",
    "test:ci": "ng test --progress=false --watch=false --code-coverage",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server",
    "set-version": "node scripts/version-from-pom.js",
    "postinstall": "npm run set-version"
  },
