{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "outDir": "./dist/out-tsc",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "module": "commonjs",
        "types": [
            "cypress",
            "node"
        ],
        "sourceMap": false
    },
    "include": [
        "**/*.ts",
        "**/*.js",
        "../cypress.config.ts",
        "../**/*.cy.ts",
        "../**/*.cy.tsx",
        "../**/*.cy.js",
        "../**/*.cy.jsx",
        "../**/*.d.ts"
    ]
}
